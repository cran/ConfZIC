year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{ConfZIC}: Confidence Envelopes for Model Selection Criteria Based on Minimum ZIC",
         author = c(person(c("I.","M.","L.","Nadeesha"), "Jayaweera"),
                    person(c("A.","Alex"), "Trindade")),
         year = 2023,
         note = note,
         url = " https://CRAN.R-project.org/package=ConfZIC")
         textVersion =
         paste0("I.M.L.Nadeesha Jayaweera,",
                "A.Alex Trindade (",
                year,
                "). ConfZIC: Confidence Envelopes for Model Selection Criteria Based on Minimum ZIC. ",
                note, ".")
